{
    "manifest_version": 2,
    "name": "PPUP",
    "description": "ChromeExtension",
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self' https://www.youtube.com;",
    "version": "1.0.1",
    "icons" : {
        "16" : "./images/16px.png",
        "32" : "./images/32px.png",
        "48" : "./images/48px.png"
    },
    "permissions":[
        "tabs"
    ],
    "background": {
        "scripts": ["./js/background.js"]
    },
    "browser_action": {
        "default_icon": {
            "16":"./images/16px.png"
        },
        "default_title": "PPUP"
    }
}
